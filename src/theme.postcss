:root {
	/* =~= Theme Styles =~= */
	--theme-border-base: 0px;
	--theme-font-family-heading: ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas,
		'Liberation Mono', 'Courier New', monospace;
	--theme-font-family-base: ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas, 'Liberation Mono',
		'Courier New', monospace;
	--theme-font-color-base: 0 0 0;
	--theme-font-color-dark: 255 255 255;
	--theme-rounded-base: 4px;
	--theme-rounded-container: 8px;
	/* =~= Theme Colors | Tailwind =~= */
	/* lime | #84cc16 */
	--color-primary-50: 247 254 231; /* ⬅ #f7fee7 */
	--color-primary-100: 236 252 203; /* ⬅ #ecfccb */
	--color-primary-200: 217 249 157; /* ⬅ #d9f99d */
	--color-primary-300: 190 242 100; /* ⬅ #bef264 */
	--color-primary-400: 163 230 53; /* ⬅ #a3e635 */
	--color-primary-500: 132 204 22; /* ⬅ #84cc16 */
	--color-primary-600: 101 163 13; /* ⬅ #65a30d */
	--color-primary-700: 77 124 15; /* ⬅ #4d7c0f */
	--color-primary-800: 63 98 18; /* ⬅ #3f6212 */
	--color-primary-900: 54 83 20; /* ⬅ #365314 */
	/* cyan | #06b6d4 */
