	--color-accent-100: 207 250 254; /* ⬅ #cffafe */
	--color-accent-200: 165 243 252; /* ⬅ #a5f3fc */
	--color-accent-300: 103 232 249; /* ⬅ #67e8f9 */
	--color-accent-400: 34 211 238; /* ⬅ #22d3ee */
	--color-accent-500: 6 182 212; /* ⬅ #06b6d4 */
